{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/react": "^5.4.2",
    "@cypress/webpack-dev-server": "^1.1.6",
    "@mermaid-js/mermaid-cli": "^8.14.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "cypress": "^7.2.0",
    "cypress-image-snapshot": "^4.0.1",
    "html-webpack-plugin": "^4.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "test-snapshot": "react-scripts test",
    "test-visual": "cypress run-ct --reporter cypress-image-snapshot/reporter --env failOnSnapshotDiff=false",
    "diagrams": "bash scripts/generate-diagrams.sh"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(snapshot).(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
